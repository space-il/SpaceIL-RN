{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    "react-native-reanimated/plugin",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@constants": "./src/constants",
          "@components": "./src/components",
          "@pages": "./src/pages"
        }
      }
    ]
  ]
}
